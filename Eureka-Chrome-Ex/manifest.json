
{
  "manifest_version": 2,
  "name": "Eureka Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.4.min.js", "content.js"]
    }
  ],

    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },

 "permissions": [
   "activeTab",
   "storage"
   ]
}